42\u0438 \u0437\u0430\u0434\u0430\u043d\u0438\u044f. \u0415\u0441\u0442\u044c \u043d\u0435 \u0431\u043e\u043b\u0435\u0435 5\u20137 \u0433\u0440\u0430\u043c\u043c\u0430\u0442\u0438\u0447\u0435\u0441\u043a\u0438\u0445 \u043e\u0448\u0438\u0431\u043e\u043a.    1\n\u0413\u0440\u0430\u043c\u043c\u0430\u0442\u0438\u0447\u0435\u0441\u043a\u0438\u0435 \u0441\u0440\u0435\u0434\u0441\u0442\u0432\u0430 \u043d\u0435 \u0441\u043e\u043e\u0442\u0432\u0435\u0442\u0441\u0442\u0432\u0443\u0435\u0442 \u0432\u044b\u0441\u043e\u043a\u043e\u043c\u0443 \u0443\u0440\u043e\u0432\u043d\u044e \u0441\u043b\u043e\u0436\u043d\u043e\u0441\u0442\u0438 \u0437\u0430\u0434\u0430\u043d\u0438\u044f. \u0415\u0441\u0442\u044c 8 \u0438 \u0431\u043e\u043b\u0435\u0435 \u0433\u0440\u0430\u043c\u043c\u0430\u0442\u0438\u0447\u0435\u0441\u043a\u0438\u0445 \u043e\u0448\u0438\u0431\u043e\u043a.          0\n\n \n\n\u041a5. \u041e\u0440\u0444\u043e\u0433\u0440\u0430\u0444\u0438\u044f \u0438 \u043f\u0443\u043d\u043a\u0442\u0443\u0430\u0446\u0438\u044f\n\u0415\u0441\u0442\u044c \u043d\u0435 \u0431\u043e\u043b\u0435\u0435 1 \u043e\u0440\u0444\u043e\u0433\u0440\u0430\u0444\u0438\u0447\u0435\u0441\u043a\u043e\u0439 \u0438\u043b\u0438 \u043f\u0443\u043d\u043a\u0442\u0443\u0430\u0446\u0438\u043e\u043d\u043d\u043e\u0439 \u043e\u0448\u0438\u0431\u043a\u0438.        2\n\u0415\u0441\u0442\u044c 2\u20134 \u043e\u0440\u0444\u043e\u0433\u0440\u0430\u0444\u0438\u0447\u0435\u0441\u043a\u0438\u0445 \u0438\u043b\u0438 \u043f\u0443\u043d\u043a\u0442\u0443\u0430\u0446\u0438\u043e\u043d\u043d\u044b\u0445 \u043e\u0448\u0438\u0431\u043e\u043a.         1\n\u0415\u0441\u0442\u044c 5 \u0431\u043e\u043b\u0435\u0435 \u043e\u0440\u0444\u043e\u0433\u0440\u0430\u0444\u0438\u0447\u0435\u0441\u043a\u0438\u0445 \u0438\u043b\u0438 \u043f\u0443\u043d\u043a\u0442\u0443\u0430\u0446\u0438\u043e\u043d\u043d\u044b\u0445 \u043e\u0448\u0438\u0431\u043e\u043a.  0\n\n \n\nII.               \u0414\u043e\u043f\u043e\u043b\u043d\u0438\u0442\u0435\u043b\u044c\u043d\u044b\u0435 \u0441\u043e\u0432\u0435\u0442\u044b:\n\n- \u041d\u0435 \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u0443\u0439\u0442\u0435 \u0440\u0438\u0442\u043e\u0440\u0438\u0447\u0435\u0441\u043a\u0438\u0435 \u0432\u043e\u043f\u0440\u043e\u0441\u044b.\n- \u041d\u0435 \u043f\u0438\u0448\u0438\u0442\u0435 \u0441\u043e\u043a\u0440\u0430\u0449\u0435\u043d\u043d\u044b\u0435 \u0432\u0430\u0440\u0438\u0430\u043d\u0442\u044b \u0430\u043d\u0433\u043b\u0438\u0439\u0441\u043a\u0438\u0445 \u0432\u044b\u0440\u0430\u0436\u0435\u043d\u0438\u0439 \u0438 \u0441\u043b\u043e\u0432. \u0422\u043e\u043b\u044c\u043a\u043e \u043f\u043e\u043b\u043d\u044b\u0435.\n- \u0418\u0437\u0431\u0435\u0433\u0430\u0439\u0442\u0435 \u044d\u043c\u043e\u0446\u0438\u043e\u043d\u0430\u043b\u044c\u043d\u043e \u043e\u043a\u0440\u0430\u0448\u0435\u043d\u043d\u044b\u0445 \u043f\u0440\u0438\u043b\u0430\u0433\u0430\u0442\u0435\u043b\u044c\u043d\u044b\u0445.\n- \u0421\u0442\u0430\u0440\u0430\u0439\u0442\u0435\u0441\u044c \u043d\u0435 \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u0442\u044c \u043a\u043e\u0440\u043e\u0442\u043a\u0438\u0435 \u043f\u0440\u0435\u0434\u043b\u043e\u0436\u0435\u043d\u0438\u044f, \u0432 \u043a\u043e\u0442\u043e\u0440\u044b\u0445 \u043c\u0435\u043d\u0435\u0435 7 \u0441\u043b\u043e\u0432.\n- \u0418\u0441\u043f\u043e\u043b\u044c\u0437\u0443\u0439\u0442\u0435 \u044d\u0441\u0441\u0435 \u043f\u043e \u0430\u043d\u0433\u043b\u0438\u0439\u0441\u043a\u043e\u043c\u0443 \u044f\u0437\u044b\u043a\u0443 \u0442\u043e\u043b\u044c\u043a\u043e \u0442\u0435 \u043b\u043e\u0433\u0438\u0447\u0435\u0441\u043a\u0438\u0435 \u0441\u0432\u044f\u0437\u043a\u0438, \u0432 \u043a\u043e\u0442\u043e\u0440\u044b\u0445 \u0443\u0432\u0435\u0440\u0435\u043d\u044b.\n- \u0427\u0435\u0442\u043a\u043e \u0441\u043b\u0435\u0434\u0443\u0439\u0442\u0435 \u043f\u043b\u0430\u043d\u0443 \u0438\u0437 \u0437\u0430\u0434\u0430\u043d\u0438\u044f.\n- \u0423\u0431\u0435\u0434\u0438\u0442\u0435\u0441\u044c, \u0447\u0442\u043e \u0432\u0441\u0435 \u0430\u0431\u0437\u0430\u0446\u044b \u0440\u0430\u0431\u043e\u0442\u044b \u043d\u0430 \u043c\u0435\u0441\u0442\u0435.\n- \u041f\u0438\u0448\u0438\u0442\u0435, \u043e\u0441\u0442\u0430\u0432\u043b\u044f\u044f \u043f\u0443\u0441\u0442\u0443\u044e \u0441\u0442\u0440\u043e\u043a\u0443 \u043c\u0435\u0436\u0434\u0443 \u0430\u0431\u0437\u0430\u0446\u0430\u043c\u0438, \u0447\u0442\u043e\u0431\u044b \u0432\u044b\u0434\u0435\u043b\u0438\u0442\u044c \u0438\u0445.\n- \u0421\u0442\u0430\u0440\u0430\u0439\u0442\u0435\u0441\u044c \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u0442\u044c \u0441\u043b\u043e\u0436\u043d\u0443\u044e \u043b\u0435\u043a\u0441\u0438\u043a\u0443, \u043a\u043e\u0442\u043e\u0440\u0430\u044f \u043f\u043e\u043a\u0430\u0436\u0435\u0442 \u0432\u0430\u0448 \u0441\u043b\u043e\u0432\u0430\u0440\u043d\u044b\u0439 \u0437\u0430\u043f\u0430\u0441.\n- \u0418\u0441\u043f\u043e\u043b\u044c\u0437\u0443\u0439\u0442\u0435 \u0432\u0432\u043e\u0434\u043d\u044b\u0435 \u0441\u043b\u043e\u0432\u0430: however, moreover, furthermore \u0438 \u0434\u0440.\n- \u041f\u0438\u0448\u0438\u0442\u0435 \u0442\u043e\u043b\u044c\u043a\u043e \u0442\u0435 \u0441\u043b\u043e\u0432\u0430, \u0432 \u0437\u043d\u0430\u0447\u0435\u043d\u0438\u0438 \u043a\u043e\u0442\u043e\u0440\u044b\u0445 \u0432\u044b \u0443\u0432\u0435\u0440\u0435\u043d\u044b.\n- \u0418\u0441\u043f\u043e\u043b\u044c\u0437\u0443\u0439\u0442\u0435 \u0441\u043b\u043e\u0436\u043d\u044b\u0435 \u0433\u0440\u0430\u043c\u043c\u0430\u0442\u0438\u0447\u0435\u0441\u043a\u0438\u0435 \u043a\u043e\u043d\u0441\u0442\u0440\u0443\u043a\u0446\u0438\u0438 \u0432 \u044d\u0441\u0441\u0435 \u043f\u043e \u0430\u043d\u0433\u043b\u0438\u0439\u0441\u043a\u043e\u043c\u0443.\n- \u0415\u0441\u043b\u0438 \u043d\u0435 \u0443\u0432\u0435\u0440\u0435\u043d\u044b \u0432 \u0442\u043e\u043c, \u043f\u0440\u0430\u0432\u0438\u043b\u044c\u043d\u043e \u043b\u0438 \u043f\u043e\u043d\u0438\u043c\u0430\u0435\u0442\u0435 \u043a\u043e\u043d\u0441\u0442\u0440\u0443\u043a\u0446\u0438\u044e, \u043b\u0443\u0447\u0448\u0435 \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u0442\u044c \u0432\u043c\u0435\u0441\u0442\u043e \u043d\u0435\u0435 \u0431\u043e\u043b\u0435\u0435 \u043f\u0440\u043e\u0441\u0442\u0443\u044e\n- \u041d\u0435 \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u0443\u0439\u0442\u0435 \u0441\u043b\u043e\u0432\u043e, \u0435\u0441\u043b\u0438 \u043d\u0435 \u043f\u043e\u043c\u043d\u0438\u0442\u0435, \u043a\u0430\u043a \u043e\u043d\u043e \u043f\u0438\u0448\u0435\u0442\u0441\u044f.\n- \u041d\u0435 \u0437\u0430\u0431\u044b\u0432\u0430\u0439\u0442\u0435 \u043e \u0437\u043d\u0430\u043a\u0430\u0445 \u043f\u0440\u0435\u043f\u0438\u043d\u0430\u043d\u0438\u044f \u0432 \u0441\u043b\u043e\u0436\u043d\u044b\u0445 \u043f\u0440\u0435\u0434\u043b\u043e\u0436\u0435\u043d\u0438\u044f\u0445 \u0438 \u0432 \u043a\u043e\u043d\u0446\u0435 \u043f\u0440\u0435\u0434\u043b\u043e\u0436\u0435\u043d\u0438\u0439.\n- \u041f\u043e\u043c\u043d\u0438\u0442\u0435, \u0447\u0442\u043e \u043d\u0435\u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u0432\u0432\u043e\u0434\u043d\u044b\u0435 \u043a\u043e\u043d\u0441\u0442\u0440\u0443\u043a\u0446\u0438\u0438 \u043d\u0443\u0436\u043d\u043e \u0432\u044b\u0434\u0435\u043b\u044f\u0442\u044c \u0437\u0430\u043f\u044f\u0442\u044b\u043c\u0438: however, on the one hand, eventually. \n \n\n### \u0418\u043d\u0441\u0442\u0440\u0443\u043a\u0446\u0438\u0438: \n- \u0414\u0430\u0439 \u043c\u0430\u043a\u0441\u0438\u043c\u0430\u043b\u044c\u043d\u043e \u0440\u0430\u0437\u0432\u0435\u0440\u043d\u0443\u0442\u0443\u044e \u043e\u0446\u0435\u043d\u043a\u0443 \u043f\u043e \u043a\u0430\u0436\u0434\u043e\u043c\u0443 \u0438\u0437 \u043a\u0440\u0438\u0442\u0435\u0440\u0438\u0435\u0432 \u043e\u0446\u0435\u043d\u043a\u0438 \u044d\u0441\u0441\u0435. \n- \u0414\u0430\u0439 \u0440\u0435\u043a\u043e\u043c\u0435\u043d\u0434\u0430\u0446\u0438\u0438, \u043a\u0430\u043a \u0443\u043b\u0443\u0447\u0448\u0438\u0442\u044c (\u043f\u0435\u0440\u0435\u0444\u0440\u0430\u0437\u0438\u0440\u043e\u0432\u0430\u0442\u044c) \u0442\u0435 \u0447\u0430\u0441\u0442\u0438, \u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u0442\u0440\u0435\u0431\u0443\u044e\u0442 \u0438\u0437\u043c\u0435\u043d\u0435\u043d\u0438\u0439 - \u043f\u0440\u043e\u0446\u0438\u0442\u0438\u0440\u0443\u0439 \u043c\u0435\u0441\u0442\u0430 \u0433\u0434\u0435 \u0435\u0441\u0442\u044c \u043e\u0448\u0438\u0431\u043a\u0438.\n- \u0414\u0430\u0439 \u043e\u0446\u0435\u043d\u043a\u0443 \u0443\u0440\u043e\u0432\u043d\u044f \u044f\u0437\u044b\u043a\u0430 \u043f\u043e \u0448\u043a\u0430\u043b\u0435 B1, B2, B3, C1 \u0438 \u0442\u0434.\n- \u0423\u043a\u0430\u0436\u0438 \u0438\u0442\u043e\u0433\u043e\u0432\u0443\u044e \u043e\u0446\u0435\u043d\u043a\u0443 \u043f\u043e \u044d\u0441\u0441\u0435.\n- \u0418\u0441\u043f\u043e\u043b\u044c\u0437\u0443\u0439 \u044f\u0437\u044b\u043a \u0440\u0430\u0437\u043c\u0435\u0442\u043a\u0438 Markdown \u0432 \u043e\u0442\u0432\u0435\u0442\u0435, \u0447\u0442\u043e\u0431\u0432\u044b \u0432\u044b\u0434\u0435\u043b\u0438\u0442\u044c \u043f\u0440\u0430\u0432\u0438\u043b\u044c\u043d\u044b\u0439 \u0442\u0435\u043a\u0441\u0442 \u0438 \u0442\u0432\u043e\u0438 \u0438\u0441\u043f\u0440\u0430\u0432\u043b\u0435\u043d\u0438\u044f.\n\n### \u0417\u0430\u0434\u0430\u043d\u0438\u0435 \u044d\u0441\u0441\u0435:\nThe internet will never replace traditional course books in school. How far do you agree with this prediction?\n\n### \u0422\u0435\u043a\u0441\u0442 \u044d\u0441\u0441\u0435:\nLast time, internet has become an integral part of our lives. Average person spend about seven hours per day by serfing the Intern, watching some videos, and chatting with friends. However, a lot of people use their phones to find knew information and upgrade their knowledge by reading something online. Some of them even participated in educational programs or some developmental courses and do not going to school or university. So thats the question. Do we really need a traditional course books in schools or internet will replace them in the nearest future? To deeply understand this question we need to refer to an education process as the base of everything. Since the world has appeared people used to read books and visit libraries to absorb information and somehow develop their sosiety. The only problem - it was incredibly hard and expensive to find an exact book that you need. As we can see now, due to intensive internet progress we can easily find anything we need. So it is the point why the internet education can replace books and school education. Nevertheless, from this it follows another problem. Internet is so considerable and countless(uncountable?) that nobody can track the authenticity of the information that is swimming it the net. In my opinion, it is better to reed traditional books and study using them, rather then surching the internet. It feels differently when you touch it and smell. That is why I would spend more time to find and more money to buy real books."}], "response": "\u041e\u0446\u0435\u043d\u0438\u043c \u0442\u0432\u043e\u0435 \u044d\u0441\u0441\u0435 \u043f\u043e \u043a\u0430\u0436\u0434\u043e\u043c\u0443 \u0438\u0437 \u043a\u0440\u0438\u0442\u0435\u0440\u0438\u0435\u0432:\n\n### \u041a1. \u0420\u0435\u0448\u0435\u043d\u0438\u0435 \u043a\u043e\u043c\u043c\u0443\u043d\u0438\u043a\u0430\u0442\u0438\u0432\u043d\u043e\u0439 \u0437\u0430\u0434\u0430\u0447\u0438\n\n\u0422\u044b \u0437\u0430\u0442\u0440\u043e\u043d\u0443\u043b \u043e\u0441\u043d\u043e\u0432\u043d\u044b\u0435 \u0430\u0441\u043f\u0435\u043a\u0442\u044b, \u043d\u043e \u043e\u0441\u0442\u0430\u0435\u0442\u0441\u044f \u0432\u043f\u0435\u0447\u0430\u0442\u043b\u0435\u043d\u0438\u0435, \u0447\u0442\u043e \u0440\u0430\u0437\u0433\u043e\u0432\u043e\u0440 \u0432\u0435\u0434\u0435\u0442\u0441\u044f \u0441\u043a\u043e\u0440\u0435\u0435 \u043e \u0432\u0430\u0436\u043d\u043e\u0441\u0442\u0438 \u0438\u043d\u0442\u0435\u0440\u043d\u0435\u0442\u0430 \u0432 \u043e\u0431\u0440\u0430\u0437\u043e\u0432\u0430\u0442\u0435\u043b\u044c\u043d\u043e\u043c \u043f\u0440\u043e\u0446\u0435\u0441\u0441\u0435, \u0447\u0435\u043c \u043e \u0441\u0440\u0430\u0432\u043d\u0435\u043d\u0438\u0438 \u0438\u043d\u0442\u0435\u0440\u043d\u0435\u0442\u0430 \u0438 \u0442\u0440\u0430\u0434\u0438\u0446\u0438\u043e\u043d\u043d\u044b\u0445 \u0443\u0447\u0435\u0431\u043d\u0438\u043a\u043e\u0432. \u0422\u0430\u043a\u0436\u0435 \u0432 \u0442\u0435\u043a\u0441\u0442\u0435 \u0435\u0441\u0442\u044c \u0444\u0440\u0430\u0437\u044b, \u043d\u0435 \u0441\u043e\u043e\u0442\u0432\u0435\u0442\u0441\u0442\u0432\u0443\u044e\u0449\u0438\u0435 \u043d\u0435\u0439\u0442\u0440\u0430\u043b\u044c\u043d\u043e\u043c\u0443 \u0441\u0442\u0438\u043b\u044e, \u043d\u0430\u043f\u0440\u0438\u043c\u0435\u0440, \"It feels differently when you touch it and smell.\" \u041e\u0446\u0435\u043d\u043a\u0430 \u043f\u043e \u044d\u0442\u043e\u043c\u0443 \u043a\u0440\u0438\u0442\u0435\u0440\u0438\u044e **2**.\n\n### \u041a2. \u041e\u0440\u0433\u0430\u043d\u0438\u0437\u0430\u0446\u0438\u044f \u0442\u0435\u043a\u0441\u0442\u0430\n\n\u0422\u0435\u043a\u0441\u0442 \u0440\u0430\u0437\u0434\u0435\u043b\u0435\u043d \u043d\u0430 \u0430\u0431\u0437\u0430\u0446\u044b, \u043d\u043e \u0441\u0432\u044f\u0437\u0438 \u043c\u0435\u0436\u0434\u0443 \u0430\u0431\u0437\u0430\u0446\u0430\u043c\u0438 \u043c\u0435\u0441\u0442\u0430\u043c\u0438 \u043a\u0430\u0436\u0443\u0442\u0441\u044f \u0441\u043b\u0430\u0431\u044b\u043c\u0438. \u0412\u0432\u0435\u0434\u0435\u043d\u0438\u0435 \u0438 \u0437\u0430\u043a\u043b\u044e\u0447\u0435\u043d\u0438\u0435 \u0435\u0441\u0442\u044c, \u043d\u043e \u043e\u043d\u0438 \u043c\u043e\u0433\u0443\u0442 \u0431\u044b\u0442\u044c \u0431\u043e\u043b\u0435\u0435 \u0447\u0435\u0442\u043a\u0438\u043c\u0438 \u0438 \u0441\u0442\u0440\u0443\u043a\u0442\u0443\u0440\u0438\u0440\u043e\u0432\u0430\u043d\u043d\u044b\u043c\u0438. \u041e\u0446\u0435\u043d\u043a\u0430 **2**.\n\n### \u041a3. \u041b\u0435\u043a\u0441\u0438\u043a\u0430\n\n\u041b\u0435\u043a\u0441\u0438\u0447\u0435\u0441\u043a\u043e\u0435 \u043e\u0444\u043e\u0440\u043c\u043b\u0435\u043d\u0438\u0435 \u0434\u043e\u0432\u043e\u043b\u044c\u043d\u043e \u043f\u0440\u043e\u0441\u0442\u043e\u0435, \u043d\u0435 \u0432\u0441\u0435\u0433\u0434\u0430 \u0441\u043e\u043e\u0442\u0432\u0435\u0442\u0441\u0442\u0432\u0443\u044e\u0449\u0435\u0435 \u0432\u044b\u0441\u043e\u043a\u043e\u043c\u0443 \u0443\u0440\u043e\u0432\u043d\u044e \u0441\u043b\u043e\u0436\u043d\u043e\u0441\u0442\u0438 \u0437\u0430\u0434\u0430\u043d\u0438\u044f. \u0415\u0441\u0442\u044c \u0438 \u043b\u0435\u043a\u0441\u0438\u0447\u0435\u0441\u043a\u0438\u0435 \u043e\u0448\u0438\u0431\u043a\u0438 (\u043d\u0430\u043f\u0440\u0438\u043c\u0435\u0440, \"serfing\" \u0432\u043c\u0435\u0441\u0442\u043e \"surfing\", \"reed\" \u0432\u043c\u0435\u0441\u0442\u043e \"read\", \"surching\" \u0432\u043c\u0435\u0441\u0442\u043e \"searching\"). \u041e\u0446\u0435\u043d\u043a\u0430 **1**.\n\n### \u041a4. \u0413\u0440\u0430\u043c\u043c\u0430\u0442\u0438\u043a\u0430\n\n\u0412 \u0442\u0435\u043a\u0441\u0442\u0435 \u0434\u043e\u0432\u043e\u043b\u044c\u043d\u043e \u043c\u043d\u043e\u0433\u043e \u0433\u0440\u0430\u043c\u043c\u0430\u0442\u0438\u0447\u0435\u0441\u043a\u0438\u0445 \u043e\u0448\u0438\u0431\u043e\u043a, \u043d\u0430\u0447\u0438\u043d\u0430\u044f \u043e\u0442 \u0432\u0440\u0435\u043c\u0435\u043d\u043d\u044b\u0445 \u0444\u043e\u0440\u043c \u0438 \u0437\u0430\u043a\u0430\u043d\u0447\u0438\u0432\u0430\u044f \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u043d\u0438\u0435\u043c \u0430\u0440\u0442\u0438\u043a\u043b\u0435\u0439. \u041d\u0430\u043f\u0440\u0438\u043c\u0435\u0440, \"Average person spend\" \u0434\u043e\u043b\u0436\u043d\u043e \u0431\u044b\u0442\u044c \"The average person spends\". \u041e\u0446\u0435\u043d\u043a\u0430 **1**.\n\n### \u041a5. \u041e\u0440\u0444\u043e\u0433\u0440\u0430\u0444\u0438\u044f \u0438 \u043f\u0443\u043d\u043a\u0442\u0443\u0430\u0446\u0438\u044f\n\n\u041e\u0448\u0438\u0431\u043a\u0438 \u0432 \u043e\u0440\u0444\u043e\u0433\u0440\u0430\u0444\u0438\u0438 (\u043d\u0430\u043f\u0440\u0438\u043c\u0435\u0440, \"knew\" \u0432\u043c\u0435\u0441\u0442\u043e \"new\", \"reed\" \u0432\u043c\u0435\u0441\u0442\u043e \"read\") \u0438 \u043f\u0443\u043d\u043a\u0442\u0443\u0430\u0446\u0438\u0438 \u0441\u043d\u0438\u0436\u0430\u044e\u0442 \u043e\u0431\u0449\u0435\u0435 \u0432\u043f\u0435\u0447\u0430\u0442\u043b\u0435\u043d\u0438\u0435 \u043e\u0442 \u0442\u0435\u043a\u0441\u0442\u0430. \u041e\u0446\u0435\u043d\u043a\u0430 **1**.\n\n#### \u0420\u0435\u043a\u043e\u043c\u0435\u043d\u0434\u0430\u0446\u0438\u0438 \u043a \u0443\u043b\u0443\u0447\u0448\u0435\u043d\u0438\u044e:\n\n1. \u0418\u0441\u043f\u0440\u0430\u0432\u0438\u0442\u044c \u043e\u0440\u0444\u043e\u0433\u0440\u0430\u0444\u0438\u0447\u0435\u0441\u043a\u0438\u0435 \u0438 \u0433\u0440\u0430\u043c\u043c\u0430\u0442\u0438\u0447\u0435\u0441\u043a\u0438\u0435 \u043e\u0448\u0438\u0431\u043a\u0438:\n   - \"serfing the Intern\" -> \"surfing the Internet\"\n   - \"knew information\" -> \"new information\"\n   - \"reed traditional books\" -> \"read traditional books\"\n   - \"rather then surching\" -> \"rather than searching\"\n\n2. \u0414\u043b\u044f \u0443\u0441\u0438\u043b\u0435\u043d\u0438\u044f \u043d\u0435\u0439\u0442\u0440\u0430\u043b\u044c\u043d\u043e\u0433\u043e \u0441\u0442\u0438\u043b\u044f \u0438\u0437\u0431\u0435\u0433\u0430\u0439 \u044d\u043c\u043e\u0446\u0438\u043e\u043d\u0430\u043b\u044c\u043d\u043e \u043e\u043a\u0440\u0430\u0448\u0435\u043d\u043d\u044b\u0445 \u0432\u044b\u0440\u0430\u0436\u0435\u043d\u0438\u0439 \u0438 \u0441\u0442\u0430\u0440\u0430\u0439\u0441\u044f \u0441\u0442\u0440\u0443\u043a\u0442\u0443\u0440\u0438\u0440\u043e\u0432\u0430\u0442\u044c \u0430\u0440\u0433\u0443\u043c\u0435\u043d\u0442\u044b \u0431\u043e\u043b\u0435\u0435 \u043b\u043e\u0433\u0438\u0447\u043d\u043e.\n\n3. \u041f\u043e\u043f\u0440\u043e\u0431\u0443\u0439 \u0431\u043